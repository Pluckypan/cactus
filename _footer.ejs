		<!-- clipboard -->

		  <script src="<%=relative_path%>lib/clipboard/clipboard.min.js"></script>
		  <script type="text/javascript">
		  $(function() {
			var btn = "<span class=\"btn-copy tooltipped tooltipped-sw\" aria-label=\"<%=language&&language.copyTo?language.copyTo:'Copy to clipboard!'%>\">";
			btn += '<i class="far fa-clone"></i>';
			btn += '</span>'; 
			$(".highlight table").before(btn);
			var clip = new ClipboardJS('.btn-copy', {
			  text: function(trigger) {
				return Array.from(trigger.nextElementSibling.querySelectorAll('.code')).reduce((str,it)=>str+it.innerText+'\n','')
			  }
			});
			clip.on('success', function(e) {
			  e.trigger.setAttribute('aria-label', "<%=language&&language.copied?language.copied:'Copied!'%>");
			  e.clearSelection();
			})
		  })
		  </script>
		  
